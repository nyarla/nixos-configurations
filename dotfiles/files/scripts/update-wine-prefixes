#!/usr/bin/env bash

set -euo pipefail

APP=/run/media/nyarla/data/local/wine
DTM=/run/media/nyarla/data/local/daw/daw

for dir in $APP $DTM/daw $DTM/plugins; do
  for prefix in $(ls "${dir}"); do
      cwd=$(pwd)

      cd "${dir}/${prefix}"
      echo $(pwd)
      wine-run wineboot -u
      wine-run wineserver -k
      cd $cwd
  done
done
