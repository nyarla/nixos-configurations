{
  "rules": {
    "rousseau": {
      "showLevels": [
        "suggestion",
        "warning",
        "error"
      ]
    },
    "alex": {
      "allow": []
    },
    "common-misspellings": {
      "ignore": []
    },
    "stop-words": {
      "defaultWords": true,
      "skip": [
        "Blockquote"
      ]
    },
    "en-capitalization": true,
    "en-spell": true,
    "unexpanded-acronym": {
      "min_acronym_len": 3
    }
  }
}
